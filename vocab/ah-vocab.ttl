#>-----------------------------------------<#
#>           Object Properties             <#
#>-----------------------------------------<#


ah:hasGPSCoordinates
	a rdf:Property,	owl:ObjectProperty;
	rdfs:comment "This property records the Gps coordinates of the central point of the excavation's location in degrees ."@en;
	rdfs:isDefinedBy <http://www.purl.com/ah/vocab/>;
	rdfs:label "Location - has GPS Coordinates"@en;
  rdfs:domain crmarchaeo:A9;
	rdfs:range geo:SpatialThing;
	vann:usageNote "Non-mandatory Property - 0..1"@en .

ah:hasResponsible
  a rdf:Property,	owl:ObjectProperty;
  rdfs:comment "This property records the responsible of an instance of Archaeologist."@en;
  rdfs:isDefinedBy <http://www.purl.com/ah/vocab/>;
  rdfs:label "Excavation - has a resposible Archaeologist"@en;
  rdfs:domain crmarchaeo:A9;
  rdfs:range foaf:Person;
  vann:usageNote "non-mandatory Property - 0..1"@en .

  ah:hasContext
  	a rdf:Property,	owl:ObjectProperty;
  	rdfs:comment "This property records the context of an instance of Context."@en;
  	rdfs:isDefinedBy <http://www.purl.com/ah/vocab/>;
  	rdfs:label "Excavation - hasCcontext"@en;
    rdfs:domain crmarchaeo:A9;
  	rdfs:range crmarchaeo:A1;
  	vann:usageNote "non-mandatory Property - 0..M"@en .

ah:hasStratigraphicVolumeUnit;
  	a rdf:Property,	owl:ObjectProperty;
  	rdfs:comment "A context has several StratigraphicVolumeUNit(CIDOC CRM A1)."@en;
  	rdfs:label "A context has several StratigraphicVolumeUNit"@en;
    rdfs:domain crmarchaeo:A1;
  	rdfs:range crmarchaeo:A2;
  	vann:usageNote "non-mandatory Property - 0..M"@en.

ah:hasTimeline
  	a rdfs:Class;
  	rdfs:comment "This property points to the specific timeline of the the layer ."@en;
  	rdfs:isDefinedBy <http://www.purl.com/ah/vocab/>;
  	rdfs:label "StratigraphicVolumeUnit - has a timeline"@en;
    rdfs:domain crmarchaeo:A2;
  	rdfs:range time:TemporalEntity
  	vann:usageNote "non-mandatory Property - o..1"@en .

ah:foundInAStratigraphicVolumeUnit
  a rdf:Property, owl:ObjectProperty;
  rdfs:comment "Points to the instances of the StratigraphicVolumeUnit of the finding."@en;
  rdfs:label "Encounter Event - an arrowhead found in a specific StratigraphicVolumeUnit"@en;
  rdfs:domain crmsci:S19;
  rdfs:range crmarchaeo:A2;
  vann:usageNote "non-mandatory Property - 0..M"@en .

ah:foundInAContext
  a rdf:Property, owl:ObjectProperty;
  rdfs:comment "Points to the instance of the Context of the finding."@en;
  rdfs:label "The Encounter Event - an arrowhead found in a specific Context"@en;
  rdfs:domain crmsci:S19;
  rdfs:range crmarchaeo:A1;
  vann:usageNote "Mandatory Property - 1..1"@en .

ah:arrowheadFindInGPSCoordinates
	a rdf:Property,	owl:ObjectProperty;
	rdfs:comment "This property points to the definition of the GPS coordinates of the location of the arrowhead in the context."@en;
	rdfs:isDefinedBy <http://www.purl.com/ah/vocab/>;
	rdfs:label "ArrowHead - gpslocation"@en;
  rdfs:domain crm:E24;
	rdfs:range geo:SpatialThing;
	vann:usageNote "Non-mandatory Property - 0..1"@en.

ah:hasMorphology
	a rdf:Property,	owl:ObjectProperty;
	rdfs:comment "This property points to an instance of the definition of the morphology of the arrowhead.."@en;
	rdfs:isDefinedBy <http://www.purl.com/ah/vocab/>;
	rdfs:label "ArrowHead - morphology"@en;
  rdfs:domain crm:E24;
	rdfs:range ah:Morphology;
	vann:usageNote "Non-mandatory Property - 0..1"@en.

ah:hasBaseShapeDetails
  	a rdf:Property,	owl:ObjectProperty;
  	rdfs:comment "This property points to an instance with the description of the base details of the arrowhead base."@en;
  	rdfs:isDefinedBy <http://www.purl.com/ah/vocab/>;
  	rdfs:label "ArrowHead - Morphology - hasBaseDetails"@en;
    rdfs:domain ah:Morphology;
  	rdfs:range ah:BaseShapeDetails;
  	vann:usageNote "Non-mandatory Property - 0..1"@en.

ah:hasTypometry
	a rdf:Property,	owl:ObjectProperty;
	dct:identifier "ah:typometry"@en;
	rdfs:comment "This property points to an instance of the definition of the typometry of the Arrowhead."@en;
	rdfs:isDefinedBy <http://www.purl.com/ah/vocab/>;
	rdfs:label "ArrowHead - typometry"@en;
  rdfs:domain crm:E24;
	rdfs:range ah:Typometry;
	vann:usageNote "Non-mandatory Property - 0..1"@en.

ah:hasChipping
  a rdf:Property,	owl:ObjectProperty;
  rdfs:comment "This Property points to an instance of the definition of the chipping of the arrowhead.."@en;
  rdfs:isDefinedBy <http://www.purl.com/ah/vocab/>;
  rdfs:label "ArrowHead - has chipping"@en;
  rdfs:domain crm:E24;
  rdfs:range ah:Chipping;
  vann:usageNote "Non mandatory Property - 0..1"@en.

