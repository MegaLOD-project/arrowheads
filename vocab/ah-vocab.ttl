ah:Morphology
	a rdfs:Class;
	rdfs:comment "Morphology of the arrowhead."@en;
	rdfs:isDefinedBy <http://www.purl.com/ah/vocab/>;
	rdfs:label "Morphology"@en.

ah:BaseShapeDetails
	a rdfs:Class;
	rdfs:comment "The arrowhead base Details."@en;
	rdfs:isDefinedBy <http://www.purl.com/ah/vocab/>;
	rdfs:label "Arrowhead-Morphology-BaseShapeDetails"@en.

ah:Typometry
  a rdfs:Class;
  rdfs:comment "Topometry of the arrowhead."@en;
  rdfs:isDefinedBy <http://www.purl.com/ah/vocab/>;
  rdfs:label "Typometry"@en.

ah:Chipping
